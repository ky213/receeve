{
  "name": "receeve",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "build": "serverless webpack --stage staging",
    "deploy:local": "serverless deploy --stage local",
    "deploy:staging": "serverless deploy --stage staging",
    "watch": "nodemon -w src -e '.*' -x 'npm run build'",
    "start": "serverless offline start --stage local",
    "test": "jest",
    "test:offline": "IS_OFFLINE=true jest"
  },
  "dependencies": {
    "source-map-support": "^0.5.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/aws-sdk": "^2.7.0",
    "@types/chai": "^4.2.21",
    "@types/jest": "^27.0.1",
    "@types/node": "^10.12.18",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^27.2.0",
    "nodemon": "^2.0.12",
    "serverless": "^2.59.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-localstack": "^0.4.34",
    "serverless-offline": "^8.2.0",
    "serverless-offline-sns": "^0.76.0",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "uuid": "^8.3.2",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
